{
  "name": "bilive_client",
  "version": "1.2.6",
  "description": "基于Node.JS的bilibili直播挂机系统",
  "main": "build/app.js",
  "engines": {
    "node": "^8.9.3",
    "npm": "^5.5.1",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "heroku-postbuild": "npm run clean && npm run build",
    "build": "npm run build:tsc && npm run build:copy",
    "build:tsc": "tsc -p tsconfig.json || exit 0",
    "build:copy": "cp ./bilive/options.default.json ./build/bilive/",
    "clean": "rm -rf build/",
    "start": "node build/app.js"
  },
  "author": "lzghzr",
  "license": "MIT",
  "devDependencies": {
  },
  "dependencies": {
    "request": "^2.83.0",
    "ws": "^3.3.2",
    "typescript": "^2.6.1",
    "@types/node": "^8.0.53",
    "@types/request": "^2.0.8",
    "@types/ws": "^3.2.0"
  }
}
